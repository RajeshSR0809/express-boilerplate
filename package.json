{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "express typescript mongoose boilerplate",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --inspect dist/index.js",
    "dev": "concurrently --kill-others \"npm:compile:watch\" \"npm:pre:dev\"",
    "dev-inspect": "nodemon --inspect ./src/index.ts" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajeshSR0809/express-boilerplate.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "mongodb",
    "mongoose",
    "typescript"
  ],
  "author": "Rajesh SR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RajeshSR0809/express-boilerplate/issues"
  },
  "homepage": "https://github.com/RajeshSR0809/express-boilerplate#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}
